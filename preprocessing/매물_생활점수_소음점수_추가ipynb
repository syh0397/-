    {
      "cell_type": "code",
      "source": [
        "# 0 ~ 3 : 12점\n",
        "# 4 ~ 6 : 24점 \n",
        "# 7 ~ 14 : 36점\n",
        "# 15 이상 : 48점 \n",
        "def food_(x):\n",
        "  if x < 4:\n",
        "    return 12\n",
        "  elif 4 <=  x <= 6:\n",
        "    return 24\n",
        "  elif 7 <=  x <= 14:\n",
        "    return 36\n",
        "  else:\n",
        "    return 48"
      ],
      "metadata": {
        "id": "_MJrqXO5053Q"
      },
      "execution_count": 69,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "def subway(x):\n",
        "  if x == 0:\n",
        "    return 0\n",
        "  elif x == 1:\n",
        "    return 15\n",
        "  elif x == 2:\n",
        "    return 30\n",
